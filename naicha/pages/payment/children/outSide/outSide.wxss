.outSide{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.outSide-wrap{
  width: 100%;
}
.add{
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
  height:100rpx;
  align-items: center;
}
.add-text{
  font-size: 30rpx;
}
.right{
  text-align: end;
}
.arrows{
  width: 40rpx;
  height: 40rpx;
}
.send{
  display: flex;
  justify-content: space-between;
  height:100rpx;
  align-items: center;
}
.send-text{
  color: #999;
  font-size: 20rpx;
}